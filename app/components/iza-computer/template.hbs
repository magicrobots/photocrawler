<div id="iza-computer" class="route-container" style={{routeContainerStyle}}>
    {{iza-computer/loading-indicator
        isVisible=isLoadingSomething}}
    <img id="vignette" class="vignette" alt="vignette" src="assets/vignette.png">
    <canvas id="source-canvas" width={{canvasWidth}} height={{canvasHeight}}></canvas>
    <canvas id="altered-canvas" width={{canvasWidth}} height={{canvasHeight}}></canvas>
    <div class="outlet-container">
        {{outlet}}
    </div>
</div>